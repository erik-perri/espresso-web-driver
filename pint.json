{
  "preset": "laravel",
  "rules": {
    "declare_strict_types": true,
    "new_with_parentheses": {
      "anonymous_class": false,
      "named_class": false
    },
    "not_operator_with_successor_space": false,
    "trailing_comma_in_multiline": {
      "elements": [
        "arguments",
        "arrays",
        "match",
        "parameters"
      ]
    }
  }
}
